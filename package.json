{
  "name": "cars",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run config -- && npm run server",
    "dev": "concurrently \"npm run server\" \"npm run dev:ssr\"",
    "build": "ng build",
    "test": "ng test",
    "server": "node index.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run cars:serve-ssr",
