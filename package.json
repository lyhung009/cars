    "build:ssr": "ng build --prod && ng run cars:server:production",
    "prerender": "ng run cars:prerender",
    "config": "ts-node ./src/config-env.ts",
    "postinstall": "npm run config -- && npm run build:ssr",
    "heroku-postbuild": "npm run serve:ssr"
  },
  "private": true,
  "dependencies": {
